{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ES2020",
    "paths": {
      "mathjs": ["./types/index.d.ts"]
    },
    "moduleResolution": "nodenext",
    "typeRoots": [],
    "types": [],
    "lib": [
      "ES2020",
      "DOM"
    ],
    "module": "NodeNext",
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": false,
    "strictFunctionTypes": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "src-wasm",
    "lib",
    "dist",
    "node_modules"
  ]
}
